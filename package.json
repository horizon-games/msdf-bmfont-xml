    "arabic-persian-reshaper": "^1.0.0",
    "canvas": "^2.3.1",
    "cli-progress": "^1.7.0",
    "commander": "^2.12.2",
    "handlebars": "^4.0.11",
    "is-invalid-path": "^0.1.0",
